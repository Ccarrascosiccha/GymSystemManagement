<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Titulos">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#133260</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="TextoGeneral">
        <item name="fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#133260</item>
    </style>
    <style name="BotonRegistrar">
        <item name="textAllCaps">false</item>
        <item name="android:textColor">#133260</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:backgroundTint">#F9C04A</item>
        <item name="cornerRadius">16dp</item>
        <item name="strokeColor">#133260</item>
        <item name="strokeWidth">2dp</item>
        <item name="icon">@drawable/ic_check</item>
        <item name="iconTint">#133260</item>
        <item name="elevation">2dp</item>
    </style>
    <style name="BotonCancelar">
        <item name="textAllCaps">false</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">15sp</item>
        <item name="android:backgroundTint">#133260</item>
        <item name="cornerRadius">16dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="icon">@drawable/ic_cancel</item>
        <item name="iconTint">#FFFFFF</item>
        <item name="elevation">2dp</item>
    </style>

    <style name="CircleImage" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>



</resources>
